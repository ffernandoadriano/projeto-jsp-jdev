CREATE DATABASE "curso-jsp"
    WITH
    OWNER = nando
    ENCODING = 'UTF8'
    LC_COLLATE = 'en_US.utf8'
    LC_CTYPE = 'en_US.utf8'
    LOCALE_PROVIDER = 'libc'
    TABLESPACE = pg_default
    CONNECTION LIMIT = -1
    IS_TEMPLATE = False;
    
CREATE TABLE login (
    login VARCHAR(50) PRIMARY KEY,
    senha VARCHAR(255) NOT NULL
); 
    
